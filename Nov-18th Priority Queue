#include <bits/stdc++.h>
using namespace std;
int main() {
    
    vector<int> vec = {1,4,6,7,3};
    
    //Max Heap
    priority_queue<int>pq;
    
    //Min Heap
    // priority_queue<int.vector<int>greater<int>>pq2;
    
    
    for(auto& x : vec){
        pq.push(x);
    }
    
    // cout << pq.top() << endl;
    
    while(pq.size()>0){
        cout << pq.top() << " ";
        pq.pop();
    }
    
    //      Operations
    // 1. push  =  O(log n);
    // 2. pop   =  O(log n);
    // 3. top   =  O(1);
    // 4. empty =  O(1);
    // 5. size  =  O(1);
    
    return 0;
}
