#include <iostream>
using namespace std;

struct Node{
    int data;
    Node* next;
    
    Node(int value){
        data = value;
        next = NULL;
    }
};

void PrintLL(Node* head){
    Node* p = head;
    while(p != NULL){
        cout << p -> data << " ";
        p = p -> next;
    }
}
int main() {
    
    Node* head = new Node(12);
    head -> next = new Node(22);
    head -> next -> next = new Node(32);
    head -> next -> next -> next = new Node(42);
    head -> next -> next -> next -> next = new Node(52);
    
    
    Node* head2 = new Node(34);
    head2 -> next = new Node(44);
    head2 -> next -> next = new Node(32);
    
    PrintLL(head);
    cout << endl;
    
    Node* p = head;
    Node* r = nullptr;
    Node* q = nullptr;
    
    while(p != nullptr){
        r = q;
        q = p;
        p = p -> next;
        q -> next = r;
    }
    
    head = q;
    
    PrintLL(head);
    
    return 0;
}
