#include <iostream>
using namespace std;

int main() {
    int arr[9] = {1,2,2,2,3,3,4,5,6};
    int target = 2;
    int lastIndex = -1; 

    for (int i = 0; i < 9; i++) {
        if (arr[i] == target) {
            lastIndex = i;
        }
    }

    if (lastIndex != -1) {
        cout << "Last occurrence is at index " << lastIndex;
    } else {
        cout << "Element does not exist";
    }

    return 0;
}
